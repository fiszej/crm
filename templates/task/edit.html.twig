{% extends "base.html.twig" %}

{% block title %}
	Edit Task
{% endblock %}


{% block content %}
	<div class="container">
		<h1>Edit Task</h1>
		{{ form_start(form) }}

		{{ form_row(form.name) }}

		{{ form_row(form.comments) }}

		{{ form_row(form.description) }}

		{{ form_row(form.time) }}

		{{ form_row(form.deadline) }}

		{{ form_row(form.customerId) }}

		{{ form_end (form) }}
	</div>

{% endblock %}
