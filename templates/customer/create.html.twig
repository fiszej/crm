{% extends "base.html.twig" %}

{% block title %} Create Customer profile {% endblock %}


{% block content %}
<div class="container">
   <h1>Create new customer</h1>
       {{ form_start(form) }}
       
            {{ form_row(form.company) }}
        
            {{ form_row(form.name) }}

            {{ form_row(form.email) }}

            {{ form_row(form.phone) }}
                
            {{ form_row(form.address) }}
                
            {{ form_row(form.zipcode) }}

            {{ form_row(form.city) }}

            {{ form_row(form.description) }}
    
        {{ form_end (form) }}
</div>

{% endblock %}
